{
    "name": "gcodego",
    "version": "0.1.0",
    "description": "A GCode sender and application.",
    "productName": "GCode Go",
    "author": "dianlight <lucio.tarantino@gmail.com>",
    "private": true,
    "scripts": {
        "lint": "eslint --ext .js,.ts,.vue ./",
        "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
        "dev": "quasar dev",
        "dev:electron": "quasar dev -m electron",
        "build": "quasar build",
        "rebuild": "electron-rebuild",
        "build:electron": "quasar build -m electron",
        "gen:patch": "patch-package --exclude '^\\.(eslintrc|nyc_output|travis\\.yml)' gcode-toolpath",
        "postinstall": "patch-package --exclude  && electron-rebuild",
        "version": "offline-github-changelog -p > CHANGELOG.md && git add CHANGELOG.md",
        "gen:icons": "icongenie generate",
        "gen:native": "electron-rebuild",
        "gen:license": "yarn icenses generate-disclaimer > DEPS_LICENSES.md",
        "check:update": "quasar upgrade"
    },
    "dependencies": {
        "@jsonforms/core": "^2.5.2",
        "@jsonforms/vue": "^2.5.2",
        "@jsonforms/vue-vanilla": "^2.5.2",
        "@quasar/extras": "^1.10.7",
        "axios": "^0.21.1",
        "camera-controls": "^1.28.4",
        "colornames": "^1.1.1",
        "core-js": "^3.14.0",
        "electron-cfg": "^1.2.6",
        "electron-default-menu": "^1.0.2",
        "electron-log": "^4.3.5",
        "electron-updater": "^4.3.10",
        "find-free-ports": "^2.0.3",
        "gcode-toolpath": "^2.2.0",
        "highlight.js": "^11.0.1",
        "json-rpc-2.0": "^0.2.16",
        "new-error": "^2.1.10",
        "quasar": "^2.0.0-rc.3",
        "split2": "^3.2.2",
        "three": "^0.129.0",
        "through2": "^4.0.2",
        "tightcnc": "link:../tightcnc",
        "url-parse": "^1.5.1",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-i18n": "^9.1.6",
        "vue3-shortkey": "^4.0.0",
        "vuex": "^4.0.1",
        "yaml": "^1.10.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.14.5",
        "@intlify/vue-i18n-loader": "^2.1.0",
        "@quasar/app": "^3.0.0-rc.3",
        "@quasar/cli": "^1.2.1",
        "@quasar/icongenie": "^2.4.0",
        "@types/colornames": "^1.1.1",
        "@types/node": "^14",
        "@types/split2": "^3.2.0",
        "@types/three": "^0.129.1",
        "@types/through2": "^2.0.36",
        "@types/url-parse": "^1.4.3",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "electron": "^13.1.2",
        "electron-builder": "^22.11.7",
        "electron-devtools-installer": "^3.2.0",
        "electron-rebuild": "^2.3.5",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-vue": "^7.11.1",
        "node-polyfill-webpack-plugin": "^1.1.3",
        "offline-github-changelog": "^2.3.0",
        "patch-package": "^6.4.7",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.4"
    },
    "browserslist": [

{
  "name": "gcodego",
  "version": "0.2.0-alpha.3",
  "description": "A GCode sender and application.",
  "productName": "GCode Go",
  "private": true,
  "author": "dianlight <lucio.tarantino@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev:site": "quasar dev",
    "dev:electron": "quasar dev -m electron -- --remote-debugging-port=9222",
    "dev": "yarn run dev:electron",
    "build": "quasar build",
    "rebuild": "electron-rebuild -m ../../../node_modules/serialport",
    "build:electron": "quasar build -m electron",
    "gen:patch": "patch-package --exclude '^\\.(eslintrc|nyc_output|travis\\.yml)' gcode-toolpath",
    "_postinstall": "patch-package --exclude  && electron-rebuild",
    "_version": "offline-github-changelog -p > CHANGELOG.md && git add CHANGELOG.md",
    "gen:icons": "icongenie generate",
    "gen:native": "electron-rebuild",
    "gen:license": "yarn icenses generate-disclaimer > DEPS_LICENSES.md",
    "check:update": "quasar upgrade"
  },
  "dependencies": {
    "@dianlight/gcodego-core": "^0.2.0-alpha.1",
    "@dianlight/plugins-autolevel": "*",
    "@dianlight/q-3d-viewer": "0.0.1",
    "@dianlight/tightcnc-core": "^0.3.2-alpha.2",
    "@jsonforms/core": "^2.5.2",
    "@jsonforms/vue": "^2.5.2",
    "@jsonforms/vue-vanilla": "^2.5.2",
    "@quasar/extras": "^1.10.11",
    "axios": "^0.21.1",
    "colornames": "^1.1.1",
    "core-js": "^3.16.3",
    "electron-cfg": "^1.2.6",
    "electron-default-menu": "^1.0.2",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.10",
    "highlight.js": "^11.2.0",
    "json-rpc-2.0": "^0.2.18",
    "new-error": "^2.1.11",
    "object-hash": "^2.2.0",
    "objtools": "^3.0.0",
    "portfinder": "^1.0.28",
    "quasar": "^2.0.4",
    "split2": "^3.2.2",
    "through2": "^4.0.2",
    "troika-three-text": "^0.42.0",
    "url-parse": "^1.5.3",
    "utils-decorators": "^1.10.1",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-i18n": "^9.1.6",
    "vue-property-decorator": "10.0.0-rc.3",
    "vue3-shortkey": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "last 2 Android versions",
    "last 2 ChromeAndroid versions",
    "last 2 FirefoxAndroid versions",
    "last 2 iOS versions",
    "last 1 Opera versions",
    "not dead"
  ],
  "engines": {
    "node": "14",
    "npm": ">= 6.14.13",
    "yarn": ">= 1.22.10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dianlight/dgcgo.git"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@manuth/typescript-eslint-plugin": "^1.4.2",
    "@quasar/app": "^3.1.0",
    "@quasar/cli": "^1.2.1",
    "@quasar/icongenie": "^2.4.0",
    "@types/colornames": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "electron": "^18.3.7",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^15.3.0",
    "electron-rebuild": "^3.2.2",
    "eslint": "^7.32.0",
    "eslint-config-es": "3.30.15",
    "eslint-plugin-quasar": "^1.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2",
    "workbox-webpack-plugin": "^6.2.4"
  }
}

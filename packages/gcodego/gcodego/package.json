{
    "name": "gcodego",
    "version": "0.1.0",
    "description": "A GCode sender and application.",
    "productName": "GCode Go",
    "author": "dianlight <lucio.tarantino@gmail.com>",
    "scripts": {
        "lint": "eslint --ext .js,.ts,.vue ./",
        "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
        "dev": "quasar dev",
        "dev:electron": "quasar dev -m electron",
        "build": "quasar build",
        "rebuild": "electron-rebuild -m ../../../node_modules/serialport",
        "build:electron": "quasar build -m electron",
        "gen:patch": "patch-package --exclude '^\\.(eslintrc|nyc_output|travis\\.yml)' gcode-toolpath",
        "_postinstall": "patch-package --exclude  && electron-rebuild",
        "version": "offline-github-changelog -p > CHANGELOG.md && git add CHANGELOG.md",
        "gen:icons": "icongenie generate",
        "gen:native": "electron-rebuild",
        "gen:license": "yarn icenses generate-disclaimer > DEPS_LICENSES.md",
        "check:update": "quasar upgrade"
    },
    "dependencies": {
        "@jsonforms/core": "^2.5.2",
        "@jsonforms/vue": "^2.5.2",
        "@jsonforms/vue-vanilla": "^2.5.2",
        "@quasar/extras": "^1.10.7",
        "axios": "^0.21.1",
        "camera-controls": "^1.28.4",
        "colornames": "^1.1.1",
        "core-js": "^3.15.1",
        "electron-cfg": "^1.2.6",
        "electron-default-menu": "^1.0.2",
        "electron-log": "^4.3.5",
        "electron-updater": "^4.3.10",
        "find-free-ports": "^2.0.3",
        "gcode-toolpath": "^2.2.0",
        "highlight.js": "^11.0.1",
        "json-rpc-2.0": "^0.2.16",
        "new-error": "^2.1.10",
        "object-hash": "^2.2.0",
        "objtools": "^3.0.0",
        "quasar": "^2.0.0",
        "split2": "^3.2.2",
        "three": "^0.129.0",
        "through2": "^4.0.2",
        "tightcnc": "file:../../tightcnc/server",
        "url-parse": "^1.5.1",
        "utils-decorators": "^1.10.0",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-i18n": "^9.1.6",
        "vue3-shortkey": "^4.0.0",
        "vuex": "^4.0.1",
        "yaml": "^1.10.2"
    },
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 Edge versions",
        "last 2 Safari versions",
        "last 2 Android versions",
        "last 2 ChromeAndroid versions",
        "last 2 FirefoxAndroid versions",
        "last 2 iOS versions",
        "last 1 Opera versions",
        "not dead"
    ],
    "engines": {
        "node": ">= 14.17.0",
        "npm": ">= 6.14.13",
        "yarn": ">= 1.22.10"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:dianlight/cncgo.git"
    },
    "devDependencies": {
        "electron-builder": "^22.4.0",
        "worker-loader": "^3.0.8"
    }
}
